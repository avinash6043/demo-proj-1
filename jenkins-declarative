pipeline{
    agent any
    environment{
    PATH= "/usr/share/maven/bin:$PATH"
    }
    stages{
        stage("Git colone"){
            steps{
                git credentialsId: 'github', url: 'https://github.com/avinash6043/demo-proj-1.git'
            }
        }
        stage("maven build"){
            steps{
                sh "mvn clean package"
            }
        }    
   }    
}
